[ignore]
.*/node_modules/.*
.*/test/.*
.*/scripts/.*
.*/examples/.*
.*/benchmarks/.*

[include]

[libs]
flow

[options]
module.name_mapper='^compiler/\(.*\)$' -> '<PROJECT_ROOT>/vueSoundCode/compiler/\1'
module.name_mapper='^core/\(.*\)$' -> '<PROJECT_ROOT>/vueSoundCode/core/\1'
module.name_mapper='^shared/\(.*\)$' -> '<PROJECT_ROOT>/vueSoundCode/shared/\1'
module.name_mapper='^web/\(.*\)$' -> '<PROJECT_ROOT>/vueSoundCode/platforms/web/\1'
module.name_mapper='^weex/\(.*\)$' -> '<PROJECT_ROOT>/vueSoundCode/platforms/weex/\1'
module.name_mapper='^server/\(.*\)$' -> '<PROJECT_ROOT>/vueSoundCode/server/\1'
module.name_mapper='^entries/\(.*\)$' -> '<PROJECT_ROOT>/vueSoundCode/entries/\1'
module.name_mapper='^sfc/\(.*\)$' -> '<PROJECT_ROOT>/vueSoundCode/sfc/\1'
suppress_comment= \\(.\\|\n\\)*\\$flow-disable-line

module.file_ext=.vue
module.file_ext=.js
module.file_ext=.jsx
